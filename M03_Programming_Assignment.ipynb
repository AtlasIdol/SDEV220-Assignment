   "source": [
    "# 9.1\n",
    "def good():\n",
    "    return ['Harry', 'Ron', 'Hermione']\n",
    "\n",
    "# 9.2\n",
    "def get_odds(first=1, last=10, step=1):\n",
    "    number = first\n",
    "    while number <= last:\n"
    "        if number % 2 != 0:\n",
    "        yield number\n",
    "        number += step\n",
    "\n",
    "odds = get_odds()\n",
    "count = 0\n",
    "for number in odds:\n",
    "    count += 1\n",
    "    if count == 3:\n",
    "        print(f'The third number from 1 to 10 is {number}')\n",
    "        break"
   ]
  }
